{
  "name": "appshot",
  "version": "1.1.0",
  "description": " A command line tool in Node.js to switch beteen apps' windows, take screenshots, and ...",
  "main": "src/api.js",
  "files": [
    "src/",
    "etc/",
    "bin/"
  ],
  "bin": {
    "appshot": "./bin/run2.js"
  },
  "scripts": {
    "test": "zoroaster test",
    "python": "python etc/run.py",
    "list": "node bin/run2",
    "chrome": "DEBUG=main bin/run2.js --app Chrome --capture --no-empty-title",
    "activechrome": "python etc/active.py --app Chrome && npm run chrome"
  },
  "keywords": [
    "app",
    "macOS",
    "screenshot",
    "screenshots"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "dependencies": {
    "cli-table": "^0.3.1",
    "debug": "^2.6.0",
    "log-rotate": "^0.2.7",
    "uuid": "^3.0.1",
    "yargs": "^6.6.0"
  },
  "devDependencies": {
    "chromedriver": "^2.27.2",
    "selenium-webdriver": "^3.0.1",
    "zoroaster": "^0.2.0"
  }
}
