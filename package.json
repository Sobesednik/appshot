{
  "name": "appshot",
  "version": "1.1.0",
  "description": " A command-line tool in Node.js to take screen-shots of different windows.",
  "main": "src/api.js",
  "files": [
    "src",
    "etc",
    "bin"
  ],
  "bin": {
    "appshot": "build/bin/index.js"
  },
  "scripts": {
    "t": "ZOROASTER_TIMEOUT=5000 zoroaster -b",
    "test": "yarn t test/spec",
    "start": "node src/bin/register",
    "python": "python etc/run.py",
    "list": "node bin/run2",
    "chrome": "DEBUG=appshot bin/run2.js --app Chrome --capture --no-empty-title",
    "c": "babel-node open_chrome",
    "lint": "eslint .",
    "build": "babel src --out-dir build --source-maps",
    "activechrome": "python etc/active.py --app Chrome && npm run chrome"
  },
  "keywords": [
    "gif",
    "macOS",
    "mac",
    "screenshot",
    "screen",
    "capture",
    "screencast"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "dependencies": {
    "cli-table": "0.3.1",
    "concat-frames": "1.0.3",
    "gif-stream": "1.1.0",
    "gifsicle": "3.0.4",
    "neuquant": "1.0.2",
    "reloquent": "0.2.0",
    "spawncommand": "2.0.1",
    "uuid": "3.2.1",
    "window-info": "1.0.2",
    "wrote": "1.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.48",
    "@babel/node": "7.0.0-beta.48",
    "@babel/plugin-syntax-object-rest-spread": "7.0.0-beta.48",
    "@babel/plugin-transform-modules-commonjs": "7.0.0-beta.48",
    "@babel/register": "7.0.0-beta.48",
    "eslint": "4.19.1",
    "eslint-config-artdeco": "1.0.0",
    "selenium-webdriver": "4.0.0-alpha.1",
    "zoroaster": "2.1.0"
  }
}
